        "fastd/routing": "^3.0",
        "fastd/swoole": "^2.2@beta",
        "fastd/utils": "^0.1",
        "fastd/testing": "^0.3",
        "catfan/Medoo": "~1.2.0",
        "symfony/cache": "~3.2.0",
        "monolog/monolog": "~1.0",
        "fastd/migration": "^0.1@beta",
        "fastd/query-builder": "^2.0@beta"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.0",
        "phpunit/dbunit": "^2.0",
